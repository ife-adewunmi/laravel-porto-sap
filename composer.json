{
    "name": "iadewunmi/laravel-porto-sap",
    "description": "This package is a solution for easy building and working with Porto pattern structure",
    "keywords": [
        "php",
        "laravel",
        "laravel-porto",
        "laravel-porto-console",
        "porto-for-laravel",
        "porto",
        "template",
        "pattern",
        "architecture"
    ],
    "homepage": "https://github.com/ife-adewunmi/laravel-porto-sap",
    "license": "MIT",
    "authors": [
        {
            "name": "Ifeoluwa Adewunmi",
            "email": "ifeoluwa.adewunmi94@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0.1"
    },
    "autoload": {
        "psr-4": {
            "Iadewunmi\\PortoSap\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Iadewunmi\\PortoSap\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Iadewunmi\\PortoSap\\PortoServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
